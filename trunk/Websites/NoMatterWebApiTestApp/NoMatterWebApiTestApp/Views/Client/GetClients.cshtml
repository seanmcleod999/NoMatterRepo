@model IEnumerable<NoMatterWebApiModels.Models.Client>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Clients</h1>

@foreach (var client in Model)
{
    <b>@client.ClientName</b>
    <br/>
    @client.ClientId
    <br/>
    @Html.ActionLink("Get Sections", "GetClientSections", "Client", new { clientId = @client.ClientId }, null)@:&nbsp;&nbsp;
    @Html.ActionLink("Get Settings", "GetClientSettings", "Client", new { clientId = @client.ClientId }, null)
    <br/><br/>
    
    
}